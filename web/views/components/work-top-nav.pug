block variables
- copy = require('app/copy.js')

nav.topnav(role="navigation")
	.nav-wrapper
		#hamburger(ngClick=toggleDrawer)
			span
			span
			span

		header.logo-wrapper
			a(href="/#")
				img(src=copy.logo.src, alt=src=copy.logo.alt).logo

		.tabs

			nav.nav
				ul
					each navItem in copy.navigation
						li(ng-class="{ selected: isSelected('#{navItem.uiSref}') }", class=navItem.className)
							a.hvr-underline-from-center(href='/#/#{navItem.uiSref}', title=navItem.title)= navItem.navTitle

